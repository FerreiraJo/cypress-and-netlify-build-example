{
  "name": "next-netlify-starter",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "build:sandbox": "env-cmd -f .env.sandbox next build",
    "start": "next start -p 4000",
    "start:test": "start-server-and-test start http://localhost:4000",
    "smoke:test": "cypress-tags run -e TAGS='@smoke' --browser chrome"
  },
  "dependencies": {
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.0.0",
    "cypress": "^8.6.0",
    "cypress-cucumber-preprocessor": "^2.5.0",
    "cypress-tags": "^0.3.0",
    "env-cmd": "^10.1.0",
    "netlify-plugin-cypress": "^2.2.0",
    "start-server-and-test": "^1.14.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
